{
  "name": "code-russ-node",
  "version": "1.0.0",
  "scripts": {
    "start": "node nodeserver/server.js",
    "test": "./node_modules/mocha/bin/mocha \"./nodeserver/tests/**/*.js\" --reporter spec",
    "test:main": "npm run test:main:istanbul && npm run test:coveralls:report",
    "test:load": "mocha ./nodeserver/tests/load/**/*.js --reporter spec",
    "test:main:blanket": "mocha --require blanket --reporter mocha-lcov-reporter ./nodeserver/tests/main/**/*.js --reporter spec && cat ./coverage/lcov.info | coveralls",
    "convert": "jscoverage nodeserver/main.js nodeserver-cov/main.js --exclude .env",
    "coveralls": "coveralls",
    "test:coveralls:report": "cat ./coverage/lcov.info | coveralls",
    "test:main:istanbul": "istanbul cover _mocha ./nodeserver/tests/main/**/*.js --reporter spec",
    "wdio:config": "wdio config",
    "wdio:test": "wdio wdiotests/wdio.conf.js",
    "test:wdio:phantomjs": "wdio wdiotests/phantomjs.wdio.conf.js",
    "test:main:istanbul:coveralls": "istanbul cover _mocha ./nodeserver/tests/main/**/*.js --reporter spec && cat ./coverage/lcov.info | coveralls",
    "mochahq:deploy2": "cd nodeserver/tests && PORT=3000 node ./../../node_modules/mochahq/server-dist/app.js",
    "mochahq:deploy": "PORT=3000 node node_modules/mochahq/server-dist/app.js"
  },
  "dependencies": {
    "apostrophe": "^2.0.0",
    "babel": "^6.5.2",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.15.2",
    "chai": "*",
    "chakram": "^1.5.0",
    "cross-env": "^1.0.6",
    "css-loader": "^0.23.1",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "file-loader": "^0.8.4",
    "http-digest-client": "0.0.3",
    "http-proxy": "^1.16.2",
    "jshint": "^2.8.0",
    "jshint-loader": "^0.8.3",
    "jsonwebtoken": "^7.2.1",
    "lodash": "^4.17.2",
    "low": "0.0.1",
    "lowdb": "^0.14.0",
    "mocha": "^3.1.2",
    "mochahq": "^1.5.1",
    "mongodb": "^2.2.13",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "node-cron": "^1.1.2",
    "node-libs-browser": "^0.5.3",
    "node-uuid": "^1.4.7",
    "nodemailer": "^2.7.0",
    "path": "^0.12.7",
    "request-digest": "^1.0.7",
    "session-file-store": "^1.0.0",
    "socket.io": "^1.7.2",
    "strip-loader": "^0.1.0",
    "surge": "^0.18.0",
    "url": "^0.11.0",
    "vue": "^2.0.0-beta.1",
    "vue-loader": "^9.0.0",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.15.0",
    "winston": "^2.3.0",
    "winston-logsene": "^1.2.1",
    "winston-papertrail": "^1.0.4",
    "www-authenticate": "^0.6.2"
  },
  "devDependencies": {
    "blanket": "^1.2.3",
    "coveralls": "^2.11.16",
    "istanbul": "^0.4.5",
    "jasmine-core": "^2.5.2",
    "jscoverage": "^0.6.0",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "mocha-lcov-reporter": "^1.2.0",
    "wdio-mocha-framework": "^0.5.8",
    "wdio-phantomjs-service": "^0.2.2",
    "wdio-spec-reporter": "0.0.5",
    "webdriverio": "^4.6.2"
  }
}